{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "flare",
      "camelcase": "Flare",
      "title": "Flare",
      "scope": "source.flare",
      "file-types": [
        "flare",
        "flr"
      ],
      "injection-regex": "^flare$",
      "class-name": "TreeSitterFlare"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Parser for the Flare language",
    "authors": [
      {
        "name": "Luke Davis",
        "email": "luke.n.davis.7@gmail.com"
      }
    ],
    "links": {
      "repository": "https://github.com/tree-sitter/tree-sitter-flare"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}